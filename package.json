{
	"name": "90dy",
	"version": "1.0.5",
	"author": "90dy <forward@90dy.ltd>",
	"repository": "https://github.com/90dy/dev.js",
	"description": "Config for Ecmascript development, based on and for developer experience.",
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"eslint",
		"tsconfig",
		"prettier",
		"editorconfig"
	],
	"release": {
		"extends": "./semantic-config"
	},
	"eslintConfig": {
		"extends": "./eslint-config"
	},
	"babel": {
		"presets": [
			"./babel-config"
		]
	},
	"commitlint": {
		"extends": "./commitlint-config"
	},
	"prettier": "./prettier-config",
	"scripts": {
		"prepare": "./scripts/prepare.sh",
		"postinstall": "./scripts/postinstall.sh",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable"
	},
	"files": [
		"browser",
		"bun",
		"node",
		"types",
		"tsconfig.json",
		"commitlint-config.js",
		"eslint-config.js",
		"prettier-config.js",
		"semantic-config.js",
		"lintstaged-config.js",
		".editorconfig"
	],
	"dependencies": {
		"@commitlint/cli": "latest",
		"@commitlint/config-conventional": "latest",
		"@semantic-release/changelog": "latest",
		"@semantic-release/commit-analyzer": "latest",
		"@semantic-release/git": "latest",
		"@semantic-release/github": "latest",
		"@semantic-release/npm": "latest",
		"@types/eslint": "latest",
		"@types/node": "latest",
		"@types/prettier": "latest",
		"@types/react": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"bun-types": "latest",
		"eslint": "latest",
		"eslint-plugin-es": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-react": "latest",
		"eslint-plugin-react-hooks": "latest",
		"eslint-plugin-react-native": "latest",
		"eslint-plugin-simple-import-sort": "latest",
		"eslint-plugin-unused-imports": "latest",
		"husky": "latest",
		"is-ci": "latest",
		"lint-staged": "latest",
		"micromatch": "latest",
		"pinst": "latest",
		"prettier": "latest",
		"semantic-release": "latest",
		"type-fest": "latest",
		"typescript": "latest",
		"webpack": "latest",
		"zod": "latest"
	}
}
